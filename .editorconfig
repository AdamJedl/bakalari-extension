# EditorConfig is awesome: https://EditorConfig.org

root = true

[*]
end_of_line = lf
insert_final_newline = true

[*.{js, ts, py, css, json, nix}]
charset = utf-8
indent_style = space
indent_size = 4
trim_trailing_whitespace = true

[{.eslintrc, .prettierrc}]
charset = utf-8
indent_style = space
indent_size = 4
trim_trailing_whitespace = true

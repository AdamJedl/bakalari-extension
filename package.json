{
  "name": "bakalari-extension",
  "version": "1.1.0",
  "description": "Average by subject, average overall, predictor, wide mode, big marks, stipendium...",
  "scripts": {
    "watch": "webpack --config webpack/webpack.dev.js --watch",
    "watch-firefox": "webpack --config webpack-firefox/webpack.dev.js --watch",
    "build": "webpack --config webpack/webpack.prod.js",
    "build-firefox": "webpack --config webpack-firefox/webpack.prod.js",
    "build-all": "webpack --config webpack/webpack.prod.js && mkdir -p dist-firefox && cp -r dist/* dist-firefox && cp manifest-firefox/manifest.json dist-firefox && python userscript-build.py",
    "style": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint --fix . --ext .ts",
    "clean": "rm -rf dist && rm -rf dist-firefox && rm -rf dist-userscript"
  },
  "author": "AdamJedl",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/chrome": "^0.0.197",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "babel-minify": "^0.5.2",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.25.0",
    "eslint-config-hardcore": "^25.1.0",
    "glob": "^8.0.3",
    "prettier": "^2.7.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "terser": "^5.15.1",
    "uglify-js": "^3.17.3"
  }
}

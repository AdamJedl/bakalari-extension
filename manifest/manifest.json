{
    "manifest_version": 3,

    "name": "__MSG_extension_name__",
    "description": "__MSG_extension_description__",
    "version": "1.2.1",
    "default_locale": "en",
    
    "content_scripts": [
        {
            "matches": ["https://*/next/prubzna.aspx"],
            "js": ["js/content_script.js"],
            "css": ["content.css"],
            "run_at": "document_end"
        },
        {
            "matches": ["https://*/next/pololetni.aspx"],
            "js": ["js/content_script_2.js"],
            "run_at": "document_end"
        }
    ],

    "permissions": [
        "storage"
    ],

    "host_permissions": [
        "https://*/next/prubzna.aspx",
        "https://*/next/pololetni.aspx"
    ]
}
